{
  "authors": [
    {
      "name": "Robert Landers",
      "email": "landers.robert@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Bottledcode\\DurablePhp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bottledcode\\DurablePhp\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "pest"
  },
  "license": "MIT",
  "name": "bottledcode/durable-php",
  "require": {
    "amphp/file": "^3.0",
    "amphp/log": "v2.0.0",
    "amphp/parallel": "^v2",
    "bottledcode/php-rql": "dev-master",
    "crell/serde": "^1.0",
    "nesbot/carbon": "^2.66",
    "php": ">=8.2",
    "ramsey/uuid": "^4.7.4",
    "withinboredom/time": "^2.1"
  },
  "suggest": {
    "ext-redis": "Required for Redis Streams",
    "ext-igbinary": "Use a better serializer"
  },
  "type": "library",
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
    "squizlabs/php_codesniffer": "^3.7",
    "pestphp/pest": "^2.8",
    "mockery/mockery": "^1.6"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "pestphp/pest-plugin": true
    }
  }
}
