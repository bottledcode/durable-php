{
    "name": "bottledcode/durable-php",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bottledcode\\DurablePhp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bottledcode\\DurablePhp\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Robert Landers",
            "email": "landers.robert@gmail.com"
        }
    ],
    "require": {
        "olvlvl/composer-attribute-collector": "^1.2",
        "php-di/php-di": "^7.0",
        "php": ">=8.2",
        "spatie/php-cloneable": "^1.0",
        "ext-parallel": "*",
        "ext-redis": "*",
        "ramsey/uuid": "^4.7",
        "withinboredom/time": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "olvlvl/composer-attribute-collector": true
        }
    }
}
